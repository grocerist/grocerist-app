{% extends "templates/partials/base.j2" %}
{% block title %}Browse Charts{% endblock title %}
{% block page_description %}
<meta name="description" content="{{ project_data.project_sub_title }}">
{% endblock page_description %}

{% block scriptHeader %}
<script src="https://code.highcharts.com/10/highcharts.js"></script>
<script src="https://code.highcharts.com/modules/drilldown.js"></script>
<script src="https://d3js.org/d3.v4.min.js"></script>
{% endblock scriptHeader %}
{% block content %}
<div class="container dashboard">
    <h1 class="text-center">
        {% include "templates/partials/link_to_top.j2" %}
        Dashboard</h1>
<div id="container religion_chart" style="width:100%; height:400px;"></div>
<div id="container categories_chart" style="width:100%; height:400px;"></div>
<div id="container time_chart" style="width:100%; height:400px;"></div>
</div>
{% endblock content%}
{% block scripts2 %}
<script src="js/dashboard.js"></script>
{% endblock scripts2 %}